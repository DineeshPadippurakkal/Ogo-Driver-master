----------> Multiple real time fire base database  under different projects


    ///Multiple real time fire base database  under different projects
    /// only one active connect can occur at once

    fun getFBRealtimeInstance(context: Context)
    {
        if(ApiConstant.REAL_TIME_FIRBASE_CONNECT == ApiConstant.REAL_TIME_FIREBASE_DB_DEFAULT_ONE )
        {
            //default db from JSON (To connect to your default databases)
            mFirebaseInstance = FirebaseDatabase.getInstance()
        }else
        {

            var fbApp:FirebaseApp

            //from google-services.json The API Key is under a property called api_key, the Application ID came from a property called mobilesdk_app_id and the database URL came from a property called firebase_url.
            // take 2nd Database url here not the default one which is present in google-services.json file.
            val options = FirebaseOptions.Builder()
                    .setApplicationId("1:659250976484:android:fc5500b78a5736d8")
                    .setApiKey("AIzaSyDkVBI9co3i8VLSwhUKobazt7gVY-qVQec")
                    .setDatabaseUrl("https://courierdriver-6b3c1-7e91d.firebaseio.com/")
                    .build()


//            FirebaseApp.initializeApp(context, options, "courierdriver-6b3c1-7e91d")
//            val secondApp = FirebaseApp.getInstance("courierdriver-6b3c1-7e91d")
//            //To connect to your different databases
//            mFirebaseInstance = FirebaseDatabase.getInstance(secondApp)


            var hasBeenInitialized = false
            val firebaseApps = FirebaseApp.getApps(context)
            for (app in firebaseApps) {
                if (app.name == "courierdriver-6b3c1-7e91d") {
                    LogUtils.error(LogUtils.TAG, " ###FIREBASE app.name : "+ app.name)
                    LogUtils.error(LogUtils.TAG, " ###FIREBASE FirebaseApp.DEFAULT_APP_NAME : "+ FirebaseApp.DEFAULT_APP_NAME)

                    hasBeenInitialized = true
                    fbApp = app
                }
            }


            if (!hasBeenInitialized)
            {

                LogUtils.error(LogUtils.TAG, " ###FIREBASE not initialized! ")
                ///here not initialised
                fbApp = FirebaseApp.initializeApp(context, options, "courierdriver-6b3c1-7e91d")
            }
            else
            {
                LogUtils.error(LogUtils.TAG, " ###FIREBASE already initialized! ")

                ///this is already initialised so just get instance of it
                fbApp = FirebaseApp.getInstance("courierdriver-6b3c1-7e91d")

            }

            mFirebaseInstance = FirebaseDatabase.getInstance(fbApp)
        }

        // get reference to 'Drivers' node
        // mFirebaseDatabase = mFirebaseInstance!!.getReference("drivers")
        mFirebaseDatabase = mFirebaseInstance!!.getReference(BuildConfig.FIREBASE_SECRATE_NODE_TEST)

    }








