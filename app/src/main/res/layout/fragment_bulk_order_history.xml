<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/activity_order_summary"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/light_bg_order_history"
	>
	
	
	<RelativeLayout
		android:id="@+id/my_order_header"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/color_them"
		
		
		>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@color/white"
			android:layout_margin="@dimen/dimen_5dp"
			android:padding="@dimen/dimen_2dp"
			>
			
			
			
			<LinearLayout
				android:id="@+id/datesLayout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				
				android:orientation="horizontal"
				android:layout_margin="@dimen/dimen_2dp">
				
				
				<FrameLayout
					android:id="@+id/StartDateLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:layout_marginStart="@dimen/dimen_20dp"
					android:layout_marginEnd="@dimen/dimen_20dp"
					android:background="@drawable/rounded_rectangle_my_orders"
					
					>
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:background="@drawable/rounded_ripple_effect"
						>
						
						<ImageView
							android:id="@+id/ivStartDate"
							android:layout_width="30dp"
							android:layout_height="30dp"
							android:layout_marginStart="@dimen/dimen_10dp"
							android:src="@drawable/ic_calendar"
							android:layout_gravity="center_vertical"
							android:padding="@dimen/dimen_5dp"
							
							/>
						
						<TextView
							android:id="@+id/tvStartDate"
							android:layout_width="match_parent"
							android:layout_height="40dp"
							android:gravity="center"
							android:text="@string/order_history_start_date"
							android:textAlignment="gravity"
							android:textAppearance="?android:attr/textAppearanceSmall"
							android:textColor="@color/white"
							android:textStyle="bold"
							android:fontFamily="@font/avenir_next_condensed_demi_bold"
							/>
					
					
					
					</LinearLayout>
				
				
				</FrameLayout>
				
				
				<FrameLayout
					android:id="@+id/EndDateLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:layout_marginStart="@dimen/dimen_20dp"
					android:layout_marginEnd="@dimen/dimen_20dp"
					android:background="@drawable/rounded_rectangle_my_orders"
					
					>
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:background="@drawable/rounded_ripple_effect"
						>
						
						<ImageView
							android:id="@+id/ivEndDate"
							android:layout_width="30dp"
							android:layout_height="30dp"
							android:layout_marginStart="@dimen/dimen_10dp"
							android:src="@drawable/ic_calendar"
							android:layout_gravity="center_vertical"
							android:padding="@dimen/dimen_5dp"
							
							/>
						
						<TextView
							android:id="@+id/tvEndDate"
							android:layout_width="match_parent"
							android:layout_height="40dp"
							android:gravity="center"
							android:text="@string/order_history_end_date"
							android:textAlignment="gravity"
							android:textAppearance="?android:attr/textAppearanceSmall"
							android:textColor="@color/white"
							android:textStyle="bold"
							android:fontFamily="@font/avenir_next_condensed_demi_bold"
							/>
					
					
					
					</LinearLayout>
				
				
				</FrameLayout>
			
			
			
			</LinearLayout>
			
			
			
			<LinearLayout
				android:id="@+id/row3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_below="@+id/datesLayout"
				android:layout_margin="@dimen/dimen_2dp"
				>
				<FrameLayout
					android:id="@+id/SearchLayout"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					
					android:layout_marginStart="@dimen/dimen_20dp"
					android:layout_marginEnd="@dimen/dimen_20dp"
					android:background="@drawable/rounded_rectangle_my_orders"
					
					>
					<RelativeLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:background="@drawable/rounded_ripple_effect"
						
						>
						
						<ImageView
							android:id="@+id/ivSearch"
							android:layout_width="30dp"
							android:layout_height="30dp"
							android:layout_marginStart="@dimen/dimen_10dp"
							android:src="@drawable/ic_search"
							android:padding="@dimen/dimen_5dp"
							android:layout_centerVertical="true"
							android:layout_alignParentStart="true"
							
							
							/>
						
						<TextView
							android:id="@+id/tvSearch"
							android:layout_width="wrap_content"
							android:layout_height="40dp"
							android:gravity="center"
							android:text="@string/order_history_search"
							android:textAlignment="gravity"
							android:textAppearance="?android:attr/textAppearanceSmall"
							android:textColor="@color/white"
							android:textStyle="bold"
							android:fontFamily="@font/avenir_next_condensed_demi_bold"
							android:layout_centerVertical="true"
							android:layout_centerHorizontal="true"
							
							
							
							/>
					
					
					
					</RelativeLayout>
				
				
				
				</FrameLayout>
			</LinearLayout>
			
			<LinearLayout
				android:id="@+id/row4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_below="@+id/row3"
				android:layout_margin="@dimen/dimen_2dp"
				
				>
				
				
				
				
				<FrameLayout
					android:id="@+id/TotalPaymentCountLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:layout_marginStart="@dimen/dimen_5dp"
					android:layout_marginEnd="@dimen/dimen_5dp"
					
					
					>
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:weightSum="2"
						
						>
						
						<TextView
							android:id="@+id/tvPaymentLable"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center"
							android:text="@string/order_history_payment_lable"
							android:textAlignment="gravity"
							android:textAppearance="?android:attr/textAppearanceSmall"
							android:textColor="@color/white"
							android:textStyle="bold"
							android:fontFamily="@font/avenir_next_condensed_regular"
							android:layout_weight="1"
							android:background="@color/colorPrimary"
							android:padding="@dimen/dimen_5dp"
							/>
						<View
							
							android:layout_width="fill_parent"
							android:layout_height="1dp"
							android:background="@color/colorYellowTheme" />
						<TextView
							android:id="@+id/tvPaymentCount"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center"
							android:text="@string/order_history_payment_count"
							android:textAlignment="gravity"
							android:textAppearance="?android:attr/textAppearanceMedium"
							android:textColor="@color/white"
							android:textStyle="bold"
							android:fontFamily="@font/avenir_next_condensed_regular"
							android:layout_weight="1"
							android:background="@color/colorPrimary"
							/>
					
					
					
					</LinearLayout>
				
				
				</FrameLayout>
				
				
				
				
				
				<FrameLayout
					android:id="@+id/TotalPaymentAmountLayout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_weight="1"
					android:layout_marginStart="@dimen/dimen_5dp"
					android:layout_marginEnd="@dimen/dimen_5dp"
					
					
					
					>
					<LinearLayout
						android:visibility="gone"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:weightSum="2"
						
						>
						<TextView
							android:id="@+id/tvPaymentTotalLable"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center"
							android:text="@string/order_history_payment_lable_amount"
							android:textAlignment="gravity"
							android:textAppearance="?android:attr/textAppearanceSmall"
							android:textColor="@color/white"
							android:textStyle="bold"
							android:fontFamily="@font/avenir_next_condensed_regular"
							android:layout_weight="1"
							android:background="@color/colorPrimary"
							android:padding="@dimen/dimen_5dp"
							/>
						
						<View
							android:layout_width="fill_parent"
							android:layout_height="1dp"
							android:background="@color/colorYellowTheme" />
						<TextView
							android:id="@+id/tvPaymentTotalAmount"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center"
							android:text="@string/order_history_payment_total_amount"
							android:textAlignment="gravity"
							android:textAppearance="?android:attr/textAppearanceMedium"
							android:textColor="@color/white"
							android:textStyle="bold"
							android:fontFamily="@font/avenir_next_condensed_regular"
							android:layout_weight="1"
							android:background="@color/colorPrimary"
							/>
					
					
					
					</LinearLayout>
				
				
				
				</FrameLayout>
			
			
			
			</LinearLayout>
		
		</RelativeLayout>
	
	</RelativeLayout>
	
	
	
	<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:fillViewport="true"
		android:layout_below="@+id/my_order_header"
		>
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/orderRecyclerView"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				
				android:layout_margin="5dp"
				
				>
			
			</androidx.recyclerview.widget.RecyclerView>
			
			<!--	<com.jjoe64.graphview.GraphView
				android:layout_width="match_parent"
				android:layout_height="200dip"
				android:id="@+id/graph"
				android:layout_below="@+id/orderRecyclerView"
				/> -->
			
		</RelativeLayout>
		
	
	
	
	</ScrollView>
	
	
	<RelativeLayout
		android:id="@+id/fragment_my_order_gif_layout"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:layout_centerInParent="true"
		
		>
		<pl.droidsonroids.gif.GifImageView
			android:id="@+id/fragment_my_order_gif_imgView"
			android:layout_width="100dp"
			android:layout_height="100dp"
			android:layout_centerHorizontal="true"
			android:layout_gravity="center"
			/>
	</RelativeLayout>
	

</RelativeLayout>

